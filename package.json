{
  "name": "jwt-generator-cli",
  "version": "1.0.3",
  "description": "Generate JWT tokens from CLI",
  "main": "jwt.ts",
  "repository": "https://github.com/hspedro/jwt-generator-cli",
  "author": "Pedro Soares <hspedro@profusion.mobi>",
  "license": "MIT",
  "private": false,
  "bin": {
    "jwt-generator": "bin/jwt.js"
  },
  "scripts": {
